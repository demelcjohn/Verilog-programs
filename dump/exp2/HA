#! /usr/bin/vvp
:ivl_version "10.3 (stable)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision + 0;
:vpi_module "system";
:vpi_module "vhdl_sys";
:vpi_module "v2005_math";
:vpi_module "va_math";
S_0x55a80cb10c30 .scope module, "halfadder_tb" "halfadder_tb" 2 1;
 .timescale 0 0;
v0x55a80cb22530_0 .var "A", 0 0;
v0x55a80cb225f0_0 .var "B", 0 0;
v0x55a80cb226c0_0 .net "C", 0 0, L_0x55a80cb229f0;  1 drivers
v0x55a80cb227c0_0 .net "S", 0 0, L_0x55a80cb22890;  1 drivers
S_0x55a80cb10db0 .scope module, "c1" "halfadder" 2 6, 3 1 0, S_0x55a80cb10c30;
 .timescale 0 0;
    .port_info 0 /INPUT 1 "A"
    .port_info 1 /INPUT 1 "B"
    .port_info 2 /OUTPUT 1 "S"
    .port_info 3 /OUTPUT 1 "C"
L_0x55a80cb22890 .functor XOR 1, v0x55a80cb22530_0, v0x55a80cb225f0_0, C4<0>, C4<0>;
L_0x55a80cb229f0 .functor AND 1, v0x55a80cb22530_0, v0x55a80cb225f0_0, C4<1>, C4<1>;
v0x55a80cadb030_0 .net "A", 0 0, v0x55a80cb22530_0;  1 drivers
v0x55a80cb22290_0 .net "B", 0 0, v0x55a80cb225f0_0;  1 drivers
v0x55a80cb22350_0 .net "C", 0 0, L_0x55a80cb229f0;  alias, 1 drivers
v0x55a80cb223f0_0 .net "S", 0 0, L_0x55a80cb22890;  alias, 1 drivers
    .scope S_0x55a80cb10c30;
T_0 ;
    %vpi_call 2 11 "$display", "Half Adder" {0 0 0};
    %vpi_call 2 13 "$dumpfile", "halfadder.vcd" {0 0 0};
    %vpi_call 2 14 "$dumpvars", 32'sb00000000000000000000000000000000, S_0x55a80cb10c30 {0 0 0};
    %vpi_call 2 15 "$monitor", "A = %b | B = %b | S = %b | C = %b", v0x55a80cb22530_0, v0x55a80cb225f0_0, v0x55a80cb227c0_0, v0x55a80cb226c0_0 {0 0 0};
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x55a80cb22530_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x55a80cb225f0_0, 0, 1;
    %delay 5, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x55a80cb225f0_0, 0, 1;
    %delay 5, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x55a80cb22530_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x55a80cb225f0_0, 0, 1;
    %delay 5, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x55a80cb225f0_0, 0, 1;
    %delay 5, 0;
    %vpi_call 2 27 "$finish" {0 0 0};
    %end;
    .thread T_0;
# The file index is used to find the file name in the following table.
:file_names 4;
    "N/A";
    "<interactive>";
    "halfadder_tb.v";
    "halfadder.v";
