#! /usr/bin/vvp
:ivl_version "10.3 (stable)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision + 0;
:vpi_module "system";
:vpi_module "vhdl_sys";
:vpi_module "v2005_math";
:vpi_module "va_math";
S_0x564f47b01750 .scope module, "and_tb1" "and_tb1" 2 1;
 .timescale 0 0;
v0x564f47b128d0_0 .net "t_out", 0 0, L_0x564f47adde00;  1 drivers
v0x564f47b129a0_0 .var "t_x", 0 0;
v0x564f47b12a70_0 .var "t_y", 0 0;
S_0x564f47b018d0 .scope module, "my_mod" "and1" 2 6, 3 1 0, S_0x564f47b01750;
 .timescale 0 0;
    .port_info 0 /INPUT 1 "x"
    .port_info 1 /INPUT 1 "y"
    .port_info 2 /OUTPUT 1 "out"
L_0x564f47adde00 .functor AND 1, v0x564f47b129a0_0, v0x564f47b12a70_0, C4<1>, C4<1>;
v0x564f47acc030_0 .net "out", 0 0, L_0x564f47adde00;  alias, 1 drivers
v0x564f47b126f0_0 .net "x", 0 0, v0x564f47b129a0_0;  1 drivers
v0x564f47b127b0_0 .net "y", 0 0, v0x564f47b12a70_0;  1 drivers
    .scope S_0x564f47b01750;
T_0 ;
    %vpi_call 2 10 "$display", "AND" {0 0 0};
    %vpi_call 2 11 "$dumpfile", "and1.vcd" {0 0 0};
    %vpi_call 2 12 "$dumpvars", 32'sb00000000000000000000000000000000, S_0x564f47b01750 {0 0 0};
    %vpi_call 2 13 "$monitor", "x = %b | y = %b | x&y = %b", v0x564f47b129a0_0, v0x564f47b12a70_0, v0x564f47b128d0_0 {0 0 0};
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x564f47b129a0_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x564f47b12a70_0, 0, 1;
    %delay 5, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x564f47b129a0_0, 0, 1;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x564f47b12a70_0, 0, 1;
    %delay 5, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x564f47b129a0_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x564f47b12a70_0, 0, 1;
    %delay 5, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x564f47b129a0_0, 0, 1;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x564f47b12a70_0, 0, 1;
    %delay 5, 0;
    %vpi_call 2 27 "$finish" {0 0 0};
    %end;
    .thread T_0;
# The file index is used to find the file name in the following table.
:file_names 4;
    "N/A";
    "<interactive>";
    "and_tb1.v";
    "and1.v";
